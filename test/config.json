
{
    "log": {
        "clear_all_logs_on_startup": true,
        "use_stdout": true,
        "directory": "./logs/",
        "filename": "log.log",
        "filter_level": "DEBUG",
        "max_bytes": 2000000,
        "backup_count": 10,
        "queue_timeout_seconds": 0.1
    },
    "Watchdog": {
        "check_period_seconds": 5
    },
    "SleepyWorker": {
        "test_key": "test_value"
    }
}
